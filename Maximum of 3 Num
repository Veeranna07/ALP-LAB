data segment
list db 01H,09H,07H
count equ 02H
data ends
code segment
ASSUME CS:CODE, DS:DATA
start:
MOV AX,DATA
MOV DS, AX
MOV SI,OFFSET list
MOV CL,COUNT
MOV AL,[SI]
AGAIN:
CMP AL, [SI+1]
JG NEXT
MOV AL, [SI+1]
NEXT:
INC SI
DEC CL
 CMP CL,0H
 JG AGAIN
MOV BL,AL
ADD AL, 30H
MOV DL,AL
MOV AH,02H
INT 21H
MOV AH, 4CH
INT 21H
CODE ENDS
END START
